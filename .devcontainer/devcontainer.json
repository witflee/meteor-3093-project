{
"name": "Nodejs with Meteor Development Environment",
"build": {
  "context": "..",
  "dockerfile": "Dockerfile"
},
"customizations": {
   "vscode": {
       "settings": {
           "terminal.integrated.shell.linux": "/bin/bash"
       },
       "extensions": [
        "vuhrmeister.vscode-meteor"
      ]
   }
},
"features": {
    "ghcr.io/devcontainers/features/github-cli:1": {}
},
"forwardPorts": [3000],
"postCreateCommand": ""
}